{
  "name": "mini-json2html",
  "description": "Tiny JSON to HTML Transpiler Library and CLI with Zero dependencies",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "module",
  "scripts": {
    "build": "npx unbuild",
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "test": "node --test --test-concurrency 1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "bin": {
    "mini-json2html": "./dist/cli.mjs",
    "json2html": "./dist/cli.mjs",
    "mj2h": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "keywords": [
    "json2html",
    "json-to-html"
  ],
  "author": "metaory <metaory@gmail.com>",
  "license": "MIT"
}
