{
  "name": "markup.json",
  "description": "HTML DOM tree representation in compact JSON -- Draft Spec, Library and CLI",
  "version": "0.0.14",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "module",
  "sideEffects": false,
  "bugs": "https://github.com/metaory/markup.json/issues",
  "homepage": "https://github.com/metaory/markup.json#readme",
  "repository": "git+https://github.com/metaory/markup.json.git",
  "scripts": {
    "build": "npx unbuild@rc",
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "test": "node --test --test-concurrency 1"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/cli.d.mts",
        "default": "./dist/cli.mjs"
      },
      "require": {
        "types": "./dist/cli.d.cts",
        "default": "./dist/cli.cjs"
      }
    }
  },
  "bin": {
    "markup": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "keywords": [],
  "author": "metaory <metaory@gmail.com>",
  "license": "MIT"
}
